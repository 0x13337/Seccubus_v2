<% %>
<h2>Edit your notification</h2>
<table class='noborder'>
	<tr><td class='noborder'>
		<label for='subject'>Subject</label> 
	</td><td class='noborder'>
		<input type='input' size=78 name='subject' id='editNotSubject' value='<%= subject %>'/>
	</td></tr>
	<tr><td class='noborder'>
		<label for='trigger'>Trigger</label> 
	</td><td class='noborder'>
		<select name='trigger' id='editNotTrigger'/></select>
	</td></tr>
	<tr><td class='noborder'>
		<label for='recipients'>Recipients</label> 
	</td><td class='noborder'>
		<input type='input' size=78 name='recipients' id='editNotRecipients' value='<%= recipients %>'/>
	</td></tr>
	<tr><td class='noborder'>
		<label for='message'>Message</label> 
	</td><td class='noborder'>
		<textarea rows=5 cols=59 name='message' id='editNotMessage'><%= message %></textarea>
	</td></tr>
	<tr><td class='noborder' colspan=2 align='right'>
		<input type='submit' value='Update Notification'/>
		<button type='button' class='cancel'>Cancel</button>
	</td></tr>
</table>
